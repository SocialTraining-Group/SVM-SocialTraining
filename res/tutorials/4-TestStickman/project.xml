      <Feature key="smhost" val="127.0.0.1"/>
      <Feature key="smport" val="8000"/>
      <Feature key="showstickmanname" val="false"/>
    </Plugin>
    <Plugin type="device" name="questionnaire" class="de.dfki.vsm.xtension.questionnaire.DialogExecutor" load="true">
      <Feature key="emotion1" val="SchÃ¼chtern"/>
